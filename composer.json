{
    "name": "nimut/testing-framework",
    "description": "TYPO3 testing framework that provides base classes and configuration for PHPUnit tests",
    "keywords": [
        "TYPO3 CMS",
        "PHPUnit",
        "testing"
    ],
    "homepage": "https://github.com/Nimut/testing-framework",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Nicole Cordes",
            "email": "typo3@cordes.co",
            "role": "Developer"
        },
        {
            "name": "Helmut Hummel",
            "email": "info@helhum.io",
            "role": "Developer",
            "homepage": "http://helhum.io"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "tests/Packages/*"
        }
    ],
    "require": {
        "php": ">= 5.5, < 7.5",
        "typo3/cms-backend": "^8.7.30 || ^9.5.13 || ^10.4 || dev-master",
        "typo3/cms-core": "^8.7.30 || ^9.5.13 || ^10.4 || dev-master",
        "typo3/cms-extbase": "^8.7.30 || ^9.5.13 || ^10.4 || dev-master",
        "typo3/cms-fluid": "^8.7.30 || ^9.5.13 || ^10.4 || dev-master",
        "typo3/cms-frontend": "^8.7.30 || ^9.5.13 || ^10.4 || dev-master",
        "typo3/cms-install": "^8.7.30 || ^9.5.13 || ^10.4 || dev-master",
        "typo3/cms-recordlist": "^8.7.30 || ^9.5.13 || ^10.4 || dev-master",
        "phpunit/phpunit": "^6.0 || ^7.0",
        "mikey179/vfsstream": "^1.6"
    },
    "require-dev": {
        "nimut/testing-framework-testbase": "@dev",
        "nimut/phpunit-merger": "^0.3"
    },
    "conflict": {
        "symfony/finder": "2.7.44 || 2.8.37 || 3.4.7 || 4.0.7",
        "typo3/cms-composer-installers": "<1.5",
        "typo3/phar-stream-wrapper": "<3.1.3"
    },
    "autoload": {
        "psr-4": {
            "Nimut\\TestingFramework\\": "src/TestingFramework/",
            "Nimut\\TestingFramework\\v87\\": "compat/v87/"
        }
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.x-dev"
        },
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "app-dir": ".Build",
            "web-dir": ".Build/public"
        }
    },
    "include-path": [
        ".Build/vendor/pear/http_request2/",
        ".Build/vendor/pear/net_url2/"
    ]
}
